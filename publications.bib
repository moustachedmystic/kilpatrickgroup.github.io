keywords = {ANIONS,APPROXIMATION,CHEMISTRY,DOUBLE-SANDWICH COMPLEX,ELECTRON-PARAMAGNETIC-RESONANCE,ENERGIES,METALLOCENES,PENTALENE,TITANOCENE,TRANSITION-METALS},
