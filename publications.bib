title = {{Realising the electrochemical stability of graphene: scalable synthesis of an ultra-durable platinum catalyst for the oxygen reduction reaction}},
url = {http://dx.doi.org/10.1039/D0NR03326J papers3://publication/doi/10.1039/D0NR03326J},
volume = {12},
