issn = {0020-1669},
journal = {Inorganic Chemistry},
number = {14},
pages = {7842 -- 7850},
title = {{Controlling the Surface Hydroxyl Concentration by Thermal Treatment of Layered Double Hydroxides}},
volume = {56},
year = {2017}
}
